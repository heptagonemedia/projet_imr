    <para styleclass="Normal"><text styleclass="Normal" style="font-weight:bold;" translate="true">Definition</text></para>
    <para styleclass="Normal"></para>
    <para styleclass="Normal"><text styleclass="Normal" style="font-weight:normal;" translate="true">void </text><text styleclass="Normal" style="font-weight:bold; color:#0000ff;" translate="true">removeSeries</text><text styleclass="Normal" style="font-weight:normal; color:#000000;" translate="true">( </text><link displaytype="text" defaultstyle="true" type="topiclink" href="EJSC.Series" styleclass="Normal" style="font-weight:normal; color:#000000;" translate="true">EJSC.Series</link><text styleclass="Normal" style="font-weight:normal; color:#000000;" translate="true"> series )</text></para>
