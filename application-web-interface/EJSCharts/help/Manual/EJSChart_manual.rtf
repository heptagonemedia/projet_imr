\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0 
\line \plain \f3\fs20 <script type="text/javascript">
\line   var chart = new EJSC.Chart(
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0     "myFirstChart", 
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0     \{ 
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0       show_legend: false,
\line       title: "My First Custom Chart"
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0     \}
