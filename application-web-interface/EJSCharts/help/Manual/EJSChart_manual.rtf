\par \pard\fi-720\li720\ql\ri0\sb0\sa0\itap1\intbl     \plain \f3\fs16 <div id="myFirstChart style="width:600; height:400;"></div>
\par \pard\fi-720\li720\ql\ri0\sb0\sa0\itap1\intbl \plain \f3\fs16\chcbpat3 
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl \plain \f3\fs16   \plain \f3\fs16   <script type="text/javascript">
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl 
\line       var chart = new EJSC.Chart(
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl         "myFirstChart", 
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl         \{
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl           show_legend: false,
\line           title: "My First Custom Chart"
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl         \}
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl       );
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl 
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl       var myChartSeries =  new EJSC.BarSeries(
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl             new EJSC.ArrayDataHandler(
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl               [
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl                 ["Month 1",1],
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl                 ["Month 2",2], 
\line                 ["Month 3",3],
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl                 ["Month 4",4],
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl                 ["Month 5",5]
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl               ]
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl             )
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl           );
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl         myChartSeries.color = 'rgb(50,210,50)';
\line         myChartSeries.lineWidth = 5;
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl 
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl       chart.addSeries(myChartSeries);
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl 
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl     \plain \f3\fs16\chcbpat3 </script>
\par \pard\fi0\li0\ql\ri0\sb0\sa0\itap1\intbl \plain \f3\fs16 
