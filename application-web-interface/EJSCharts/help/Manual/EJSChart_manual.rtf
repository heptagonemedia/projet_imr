\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0         new EJSC.ArrayDataHandler(
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0           [
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0             ["Month 1",1],
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0             ["Month 2",2], 
\line             ["Month 3",3],
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0             ["Month 4",4],
\par \pard\fi0\li720\ql\ri0\sb0\sa0\itap0             ["Month 5",5]
